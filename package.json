{
  "name": "secrets-manager",
  "version": "2.0.0",
  "description": "NodeJS config decryptor for AWS Secrets Manager",
  "author": "Ash Brener",
  "main": "/",
  "types": "./lib/index.d.ts",
  "scripts": {
    "test": "jest",
    "type-check": "tsc --noEmit",
    "type-check:watch": "npm run type-check -- --watch",
