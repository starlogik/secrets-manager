  "dependencies": {
    "aws-sdk": "^2.809.0",
    "graceful-fs": "^4.2.4",
    "writeln": "^2.0.3"
  },
  "devDependencies": {
    "@babel/cli": "^7.12.10",
    "@babel/core": "^7.12.10",
    "@babel/preset-env": "^7.12.10",
    "@babel/preset-typescript": "^7.12.7",
    "@types/jest": "^26.0.19",
    "@types/node": "^14.14.13",
    "@types/graceful-fs": "^4.1.4",
    "babel-jest": "^26.6.3",
    "jest": "^26.6.3",
    "typescript": "^4.1.3"
  }
